{
  "name": "sockbasher",
  "version": "0.0.1",
  "description": "test websocket data delivery resiliance",
  "main": "./lib/index.js",
  "bin": {
    "sockbasher": "./lib/index.js"
  },
  "scripts": {
    "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
