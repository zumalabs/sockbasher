  "dependencies": {
    "chalk": "^4.1.1",
    "clear": "^0.1.0",
    "commander": "^7.2.0",
    "figlet": "^1.5.0",
    "path": "^0.12.7",
    "graphql-request": "^3.4.0"
  },
  "devDependencies": {
    "@types/clear": "^0.1.1",
    "@types/figlet": "^1.5.1",
